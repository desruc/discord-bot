{
  "name": "king-croc-bot",
  "version": "1.0.0",
  "description": "a basic discord bot",
  "main": "index.js",
  "repository": "https://github.com/desruc/king-croc-bot.git",
  "author": "desruc",
  "license": "MIT",
  "scripts": {
    "watch": "node_modules/.bin/tsc -w",
    "tsc": "node_modules/.bin/tsc",
    "build": "rimraf dist && npm run tsc",
    "start": "yarn build && node dist/index.js",
    "lint": "eslint --fix --ignore-path .gitignore . --ext ts --ext js",
    "dev": "npm run build && npm run start"
  },
  "dependencies": {
    "@types/node-schedule": "^1.3.1",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "node-schedule": "^2.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  }
}
